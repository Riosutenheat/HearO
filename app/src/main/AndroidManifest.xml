<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.junseo.test03">
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".RegistActivity"></activity>
        <activity android:name=".MenuActivity" android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".FindpasswordActivity"></activity>
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".HelpActivity"></activity>
        <activity android:name=".VersionActivity"></activity>
        <activity android:name=".AlarmActivity"></activity>
        <activity android:name=".MacroActivity"></activity>
        <activity android:name=".TTSActivity"></activity>
        <activity android:name=".STTActivity"></activity>
        <!--<activity android:name=".arduino.BluetoothPairActivity"></activity>-->
    </application>

</manifest>